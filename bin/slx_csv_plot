#!/bin/bash
NUM_LINES=1000
APP=audio_filter
EXY_DIR=apps/$APP/exynos

# be sure to always operate in the right directory
script_dir=$(dirname $0)
cd "$script_dir"
cd ..

# create csv file
xzcat $EXY_DIR/../exhaustive_exynos.csv.xz | head -n $NUM_LINES > $EXY_DIR/exynos_exhaustive_first_${NUM_LINES}_lines.csv


python3 -m pykpn.slx.scripts.csv_plot  $EXY_DIR/exynos_exhaustive_first_${NUM_LINES}_lines.csv $EXY_DIR/config.ini $APP
